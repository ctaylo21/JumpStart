{
  "name": "typescriptreactboilerplate",
  "version": "2.0.0",
  "description": "Typescript & React boilerplate with Webpack, Eslint, Prettier, and Jest",
  "repository": {
    "type": "git",
    "url": "https://github.com/ctaylo21/TypescriptReactBoilerplate.git"
  },
  "keywords": [
    "typescript",
    "react",
    "jest",
    "prettier",
    "webpack",
    "eslint"
  ],
  "author": "Caleb Taylor <caleb89taylor@gmail.com>",
  "license": "MIT",
  "main": "dist/main.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint"
    ],
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --open --mode=development",
    "build": "webpack --mode=production",
    "cm": "git-cz",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/react-test-renderer": "^16.8.1",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "clean-webpack-plugin": "^2.0.1",
    "commitizen": "^3.0.7",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "path": "^0.12.7",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "ts-jest": "^24.0.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tslib": "^1.9.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
