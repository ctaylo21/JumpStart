{
  "name": "jumpstart",
  "version": "0.0.6",
  "description": "ES6 module starter kit with Webpack, Babel, Mocha, Chai, Sinon, Sass, and Istanbul",
  "repository": {
    "type": "git",
    "url": "https://github.com/ctaylo21/JumpStart.git"
  },
  "keywords": [
    "es6",
    "javascript",
    "mocha",
    "chai",
    "sinon",
    "webpack",
    "babel",
    "istanbul",
    "standardjs"
  ],
  "author": "Caleb Taylor <caleb89taylor@gmail.com>",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack/dev.js",
    "build": "webpack --progress --config webpack/prod.js",
    "test": "cross-env NODE_ENV=test npx nyc mocha-webpack",
    "tdd": "mocha-webpack --watch",
    "tdd:web": "webpack-dev-server --open --config webpack/test.js",
    "lint": "standard",
    "stats": "webpack --progress --config webpack/stats.js"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "test"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "13.1.0",
    "jsdom-global": "3.0.2",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "mocha-webpack": "^2.0.0-beta.0",
    "node-sass": "^4.11.0",
    "nyc": "^13.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "path": "^0.12.7",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.2",
    "standard": "^12.0.1",
    "webpack": "^4.28.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "standard": {
    "globals": [
      "describe",
      "beforeEach",
      "it",
      "afterEach",
      "assert",
      "sinon"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0"
  }
}
